# Defaults:
ac_help=
ac_default_prefix=/usr/local
# Any additions from configure.in:
ac_help="$ac_help
  --enable-shared[=PKGS]  build shared libraries [default=yes]"
ac_help="$ac_help
  --enable-static[=PKGS]  build static libraries [default=yes]"
ac_help="$ac_help
  --enable-fast-install[=PKGS]  optimize for fast installation [default=yes]"
ac_help="$ac_help
  --with-gnu-ld           assume the C compiler uses GNU ld [default=no]"
ac_help="$ac_help
  --disable-libtool-lock  avoid locking (might break parallel builds)"
ac_help="$ac_help
  --enable-debug          build for debugging (default=no)"
ac_help="$ac_help
  --enable-optimize       build with optimization (default=no)"
ac_default_prefix=${NTOPDIR:-/usr/local}
ac_help="$ac_help
  +----------------------------------------------------------------------------+"
ac_help="$ac_help
  --with-pcap-root=DIR    LBNL pcap source tree located in DIR"
ac_help="$ac_help
  --with-gdbm-root=DIR    GNU gdbm source tree located in DIR"
ac_help="$ac_help
  --with-ossl-root=DIR    OpenSSL source tree located in DIR"
ac_help="$ac_help
  +----------------------------------------------------------------------------+"
ac_help="$ac_help
  --disable-mt            disable multithread support also if available [default=no]"
ac_help="$ac_help
  --disable-ssl           disable HTPPS support also if OpenSSL is available [default=no]"
